		<Storyboard x:Name="Str_PictureZoomPopUpStarts">
			<DoubleAnimation Duration="0:0:0.3" Storyboard.TargetName="Trnsfm_ImgZoomPopup"  Storyboard.TargetProperty="ScaleX" To="1" />
			<DoubleAnimation Duration="0:0:0.3" Storyboard.TargetName="Trnsfm_ImgZoomPopup"  Storyboard.TargetProperty="ScaleY" To="1" />
		</Storyboard>
		
		<Storyboard x:Name="Str_PictureZoomPopUpCloses">
