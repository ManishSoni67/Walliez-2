			<DoubleAnimation Duration="0:0:0.2" Storyboard.TargetName="Trnsfm_ImgZoomPopup"  Storyboard.TargetProperty="ScaleY" To="0" />
		</Storyboard>
		<Storyboard x:Name="Str_NatureNotificationShow">
