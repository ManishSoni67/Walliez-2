			</Storyboard>
		<Storyboard x:Name="Str_PageTrnsleft">
    <DoubleAnimation BeginTime="0:0:0" Duration="0:0:0.3" Storyboard.TargetName="PageTrnsfrm2" Storyboard.TargetProperty="GlobalOffsetZ" To="-300"/>
	<DoubleAnimation BeginTime="0:0:0.3" Duration="0:0:0.3" Storyboard.TargetName="PageTrnsfrm" Storyboard.TargetProperty="TranslateX" To="600" />
			</Storyboard>
			
		<Storyboard x:Name="Str_PageTrnsBack">
    <DoubleAnimation BeginTime="0:0:0.3" Duration="0:0:0.3" Storyboard.TargetName="PageTrnsfrm2" Storyboard.TargetProperty="GlobalOffsetZ" To="0"/>
	<DoubleAnimation BeginTime="0:0:0" Duration="0:0:0.3" Storyboard.TargetName="PageTrnsfrm" Storyboard.TargetProperty="TranslateX" To="0" />
			</Storyboard>
			
		<Storyboard x:Name="Str_PageTrnsRight">
    <DoubleAnimation BeginTime="0:0:0" Duration="0:0:0.3" Storyboard.TargetName="PageTrnsfrm2" Storyboard.TargetProperty="GlobalOffsetZ" To="-300"/>
	<DoubleAnimation BeginTime="0:0:0.3" Duration="0:0:0.3" Storyboard.TargetName="PageTrnsfrm" Storyboard.TargetProperty="TranslateX" To="-600" />
			</Storyboard>
			<Storyboard x:Name="SplashScreenStartUp">
				<DoubleAnimation Duration="0:0:1" Storyboard.TargetName="Rot_Googlert_Logo" Storyboard.TargetProperty="RotationY" To="0" />
				<DoubleAnimation Duration="0:0:2" Storyboard.TargetName="TrsfrmSplashLogo" Storyboard.TargetProperty="ScaleX" To="1" />
				<DoubleAnimation Duration="0:0:2" Storyboard.TargetName="TrsfrmSplashLogo" Storyboard.TargetProperty="ScaleY" To="1" />
				</Storyboard>
				<Storyboard x:Name="ApplicationLoadsUp">
				<DoubleAnimation BeginTime="0:0:1" Duration="0:0:1" Storyboard.TargetName="Rot_Googlert_Logo" Storyboard.TargetProperty="RotationY" To="90" />
				
				<DoubleAnimation BeginTime="0:0:1" Duration="0:0:1" Storyboard.TargetName="TrsfrmSplashLogo" Storyboard.TargetProperty="TranslateX" To="900" />
				<DoubleAnimation BeginTime="0:0:2" Duration="0:0:1" Storyboard.TargetName="Brd_SpashScreen" Storyboard.TargetProperty="Opacity" To="0" />
				<DoubleAnimation BeginTime="0:0:2" Duration="0:0:0.5" Storyboard.TargetName="TrsfrmSplashScreen" Storyboard.TargetProperty="TranslateX" To="-500" />
				<DoubleAnimation BeginTime="0:0:2.5" Duration="0:0:0.5" Storyboard.TargetName="TrnsfrmAppLogo" Storyboard.TargetProperty="TranslateX" To="0" />
			     </Storyboard>
				 <Storyboard x:Name="ShowPopupBtnPanel">
					<DoubleAnimation BeginTime="0:0:0" Duration="0:0:0.3" Storyboard.TargetName="Grd_PopUp_MenuPanel" Storyboard.TargetProperty="Width" To="469" />
					</Storyboard>
				 <Storyboard x:Name="HidePopupBtnPanel">
					<DoubleAnimation BeginTime="0:0:0" Duration="0:0:0.2" Storyboard.TargetName="Grd_PopUp_MenuPanel" Storyboard.TargetProperty="Width" To="50" />	
					</Storyboard>
				<Style x:Key="Thumb_Style_Template" TargetType="Thumb">
					<Setter Property="Background" Value="{StaticResource PhoneRadioCheckBoxBrush}"/>
					<Setter Property="BorderBrush" Value="{StaticResource PhoneRadioCheckBoxBrush}"/>
					<Setter Property="BorderThickness" Value="{StaticResource PhoneBorderThickness}"/>
					<Setter Property="Height" Value="48"/>
					<Setter Property="Width" Value="48"/>
				</Style>
				<ControlTemplate x:Key="Thumb_Template_Style" TargetType="Thumb">
					<Border Background="Transparent">
						<VisualStateManager.VisualStateGroups>
							<VisualStateGroup x:Name="CommonStates">
								<VisualState x:Name="Normal"/>
								<VisualState x:Name="MouseOver"/>
								<VisualState x:Name="Pressed">
									<Storyboard>
										<DoubleAnimation Duration="0" To="1" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Border" d:IsOptimized="True"/>
									</Storyboard>
								</VisualState>
								<VisualState x:Name="Disabled">
									<Storyboard>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="Background">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource PhoneDisabledBrush}"/>
										</ObjectAnimationUsingKeyFrames>
										<ObjectAnimationUsingKeyFrames Storyboard.TargetName="Border" Storyboard.TargetProperty="BorderBrush">
											<DiscreteObjectKeyFrame KeyTime="0" Value="{StaticResource PhoneDisabledBrush}"/>
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</VisualState>
							</VisualStateGroup>
							<VisualStateGroup x:Name="FocusStates"/>
						</VisualStateManager.VisualStateGroups>
						<Border x:Name="Border" IsHitTestVisible="False" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" Opacity="0.5" />
					</Border>
				</ControlTemplate>
		
	</phone:PhoneApplicationPage.Resources>
	<phone:PhoneApplicationPage.Foreground>
		<StaticResource ResourceKey="PhoneAccentBrush"/>
	</phone:PhoneApplicationPage.Foreground>
	<phone:PhoneApplicationPage.Projection>
		<PlaneProjection x:Name="PageTrnsfrm2" GlobalOffsetZ="0" CenterOfRotationX="0.5" CenterOfRotationY="0.5"/>
	</phone:PhoneApplicationPage.Projection>
	<phone:PhoneApplicationPage.RenderTransform>
		<CompositeTransform x:Name="PageTrnsfrm" TranslateX="0"/>
	</phone:PhoneApplicationPage.RenderTransform>

  <phone:PhoneApplicationPage.ApplicationBar>
    <shell:ApplicationBar x:Name="Application_Bar" IsVisible="True" IsMenuEnabled="False" Mode="Default" Opacity="0.785" BackgroundColor="{StaticResource PhoneAccentColor}">
        <shell:ApplicationBarIconButton x:Name="Btn_Rate" IconUri="/Assets/AppBar/appbar.favs.rest.png" Text="Rate Us" Click="Btn_Rate_Click"/>
        <shell:ApplicationBarIconButton x:Name="Btn_ShuffleImages" IconUri="/Assets/AppBar/appbar.refresh.rest.png" Text="Shuffle" Click="Btn_ShuffleImages_Click"/>
        <shell:ApplicationBarIconButton x:Name="Btn_AboutApp" IconUri="/Assets/AppBar/appbar.questionmark.rest.png" Text="Dev." Click="Btn_AboutApp_Click"/>
    </shell:ApplicationBar>
</phone:PhoneApplicationPage.ApplicationBar>
	<Grid x:Name="LayoutRoot" Background="{StaticResource PhoneAccentBrush}">
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
		<Popup x:Name="Popup" Grid.RowSpan="2" RenderTransformOrigin="0.5,0.5" IsOpen="True">
			<Popup.RenderTransform>
				<CompositeTransform x:Name="Trnsfm_ImgZoomPopup" ScaleX="0" ScaleY="0"/>
			</Popup.RenderTransform>
			<Grid>
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="*" />
					<ColumnDefinition Width="Auto" />
					</Grid.ColumnDefinitions>
				<ScrollViewer Grid.ColumnSpan="2">
					<Image x:Name="Img_ZoomPopup" Source="/Wallpapers/Nature/Image (1).jpg"/>
				</ScrollViewer>
				<Grid x:Name="Grd_PopUp_MenuPanel" Width="50"  MinWidth="50"  Height="100" Canvas.ZIndex="1" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Top" Background="#FFFFE800">
				<Grid.ColumnDefinitions>
					<ColumnDefinition Width="50" />
					<ColumnDefinition Width="Auto" />
					</Grid.ColumnDefinitions>
				<Thumb Name="Thmb_Panel" Grid.Column="0" Width="50" Background="#FF78FF08" DragDelta="Thmb_Panel_DragDelta"  Height="Auto" Canvas.ZIndex="1" BorderThickness="1" Style="{StaticResource Thumb_Style_Template}" Template="{StaticResource Thumb_Template_Style}" DoubleTap="Thmb_Panel_DoubleTap" >
					</Thumb>
