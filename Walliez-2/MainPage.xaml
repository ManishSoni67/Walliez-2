			</Storyboard>
			
		<Storyboard x:Name="Str_NatureNotificationClose">
			<DoubleAnimation BeginTime="0:0:2" Duration="0:0:0.2" Storyboard.TargetName="Trns_NatureNotification" Storyboard.TargetProperty="TranslateX" To="500" />
			</Storyboard>
			
